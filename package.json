{
  "name": "hacker-rank",
  "version": "0.0.0",
  "description": "Submissions of HackerRank problems written in TypeScript",
  "main": "index.js",
  "author": "somallg",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "compile": "gulp compile",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "test": "gulp test --headless"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "gulp lint"
    }
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/condition-travis"
    ],
    "getLastRelease": "@semantic-release/git",
    "publish": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "5.2.8",
    "@commitlint/config-angular": "^6.0.2",
    "@semantic-release/changelog": "1.0.0",
    "@semantic-release/git": "2.0.1",
    "@types/chalk": "2.2.0",
    "@types/del": "3.0.0",
    "@types/gulp": "3.8.36",
    "@types/gulp-if": "0.0.33",
    "@types/gulp-load-plugins": "0.0.31",
    "@types/gulp-typescript": "2.13.0",
    "@types/jasmine": "2.8.3",
    "@types/karma": "1.7.3",
    "@types/yargs": "10.0.1",
    "chalk": "2.3.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "del": "^3.0.0",
    "gulp": "3.9.1",
    "gulp-file": "^0.4.0",
    "gulp-if": "2.0.2",
    "gulp-karma": "0.0.5",
    "gulp-load-plugins": "1.5.0",
    "gulp-print": "2.0.1",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "3.2.3",
    "husky": "0.14.3",
    "jasmine-core": "2.8.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "2.0.9",
    "phantomjs-prebuilt": "2.1.16",
    "semantic-release": "11.0.2",
    "semantic-release-cli": "3.5.0",
    "standard-version": "4.3.0",
    "ts-loader": "3.2.0",
    "ts-node": "4.1.0",
    "tslint": "5.8.0",
    "tslint-config-airbnb": "5.4.2",
    "typescript": "2.6.2",
    "webpack": "3.10.0",
    "yargs": "10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/somallg/hacker-rank.git"
  }
}
